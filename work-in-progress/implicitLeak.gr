data Bool where
  True : Bool;
  False : Bool

leak : Bool [Private] → Bool [Public]
leak [x] =
  case x of
    True  →  [True];
    False →  [False]

leak' : Bool [Private] → Bool [Public]
leak' x =
      case x of
        [True]  →  [True];
        [False] →  [False]

okay : Bool [Private] → Bool [Private]
okay x =
      case x of
        [True]  →  [True];
        [False] →  [False]

